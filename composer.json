{
    "name": "gmusa/cakephp-subdomains",
    "description": "Easily add sub domains to your CakePHP application using route prefixes.",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "cakephp3", "subdomains"],
    "homepage": "https://github.com/gmusa/cakephp-subdomains",
    "license": "MIT",
    "authors": [{
        "name": "gmusa",
        "homepage": "https://gmusa.gmusa",
        "role": "Developer"
    }],
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": "^3.3",
        "cakephp/cakephp-codesniffer": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.6|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Multidimensional\\Subdomains\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Multidimensional\\Subdomains\\Test\\": "tests"
        }
    }
}